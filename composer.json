{
  "name": "webifycms/ext-admin",
  "description": "WebifyCMS admin extension",
  "type": "webifycms-extension",
  "authors": [
    {
      "name": "Mohammed Shifreen",
      "email": "mshifreen@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "yiisoft/yii2": "~2.0",
    "webifycms/ext-base": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "webifycms/dev-tools": "*"
  },
  "autoload": {
    "psr-4": {
      "Webify\\Admin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Webify\\Admin\\Test\\": "test"
    }
  },
  "optimize-autoloader": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/webifycms/ext-base"
    },
    {
      "type": "vcs",
      "url": "https://github.com/webifycms/dev-tools"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}